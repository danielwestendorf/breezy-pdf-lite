{
  "name": "breezy-pdf-lite",
  "version": "0.0.1",
  "description": "Render HTML to a PDF with Google Chrome, via an HTTP request",
  "main": "index.js",
  "author": "Daniel Westendorf",
  "license": "GNUv3",
  "dependencies": {
    "body-parser": "^1.18.3",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "html-pdf-chrome": "^0.5.0",
    "jsdom": "^15.1.1"
  },
  "devDependencies": {
    "coveralls": "^3.0.5",
    "eslint": "^6.4.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^3.0.1",
    "istanbul": "^0.4.5",
    "node-fetch": "^2.6.0",
    "nyc": "^14.1.1",
    "teenytest": "^5.2.0",
    "testdouble": "^3.12.2"
  },
  "scripts": {
    "test": "teenytest 'test/**/*-test.js' --helper test/helper.js && ./node_modules/.bin/eslint *.js, **/**.js",
